{
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noImplicitOverride": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true
  }
}